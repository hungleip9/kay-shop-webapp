;AAqJXwB,EAAAA,OArJW,mBAqJHd,EArJG,EAqJC;AACR,WAAOtC,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,MADI;AAEZC,MAAAA,GAAG,EAAE,qBAAqBU;AAFd,KAAD,CAAf;AAIH,GA1JU;AA2JXe,EAAAA,aA3JW,yBA2JGf,EA3JH,EA2JOR,IA3JP,EA2Ja;AACpB,WAAO9B,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,KADI;AAEZC,MAAAA,GAAG,EAAE,qBAAqBU,EAFd;AAGZR,MAAAA,IAAI,EAAEA;AAHM,KAAD,CAAf;AAKH,GAjKU;AAkKXwB,EAAAA,UAlKW,sBAkKAhB,EAlKA,EAkKI;AACX,WAAOtC,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,QADI;AAEZC,MAAAA,GAAG,EAAE,YAAYU;AAFL,KAAD,CAAf;AAIH,GAvKU;AAwKXiB,EAAAA,UAxKW,sBAwKAzB,IAxKA,EAwKM;AACb,WAAO9B,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,MADI;AAEZC,MAAAA,GAAG,EAAE,oBAFO;AAGZE,MAAAA,IAAI,EAAEA;AAHM,KAAD,CAAf;AAKH,GA9KU;AA+KX0B,EAAAA,aA/KW,yBA+KGd,MA/KH,EA+KW;AAClB,WAAO1C,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,KADI;AAEZC,MAAAA,GAAG,EAAE,SAFO;AAGZc,MAAAA,MAAM,EAAEA;AAHI,KAAD,CAAf;AAKH,GArLU;AAsLXe,EAAAA,kBAtLW,8BAsLQnB,EAtLR,EAsLYR,IAtLZ,EAsLkB;AACzB,WAAO9B,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,KADI;AAEZC,MAAAA,GAAG,EAAE,2BAA2BU,EAFpB;AAGZR,MAAAA,IAAI,EAAEA;AAHM,KAAD,CAAf;AAKH,GA5LU;AA6LX;AACA4B,EAAAA,kBA9LW,8BA8LQ5B,IA9LR,EA8Lc;AACrB,WAAO9B,QAAQ,CAAC;AACZ2B,MAAAA,MAAM,EAAE,MADI;AAEZC,MAAAA,GAAG,EAAE,kBAFO;AAGZE,MAAAA,IAAI,EAAEA;AAHM,KAAD,CAAf;AAKH;AApMU,CAAf","sourcesContent":["import axios from 'axios'\nimport store from \"../store\";\nimport router from \"../router\";\n\nconst baseUrl = process.env.VUE_APP_BASE_URL;\n\nexport const apiAxios = axios.create({\n    baseURL: `${baseUrl}/api`,\n    headers: {\n        post: {\n            'Content-Type': 'application/json'\n        }\n    }\n})\n\napiAxios.interceptors.request.use(config => {\n    let token = store.state.auth.accessToken\n    if (token) {\n        config.headers.common['Authorization'] = `Bearer ${token}`\n    }\n    return config\n}, error => {\n    return Promise.reject(error)\n})\n\napiAxios.interceptors.response.use(undefined, (error) => {\n    if (error) {\n        const originalRequest = error.config;\n        if (error.response.status === 401 && !originalRequest._retry && router.currentRoute.name !== 'Login') {\n            store.commit('auth/updateLoginStatus', false)\n            store.commit('auth/updateAuthUser', {})\n            store.commit('auth/updateAccessToken', '')\n            return router.push({ name: 'Login' })\n        }\n    }\n    return Promise.reject(error);\n})\n\nexport default {\n    //lay du lieu profile\n    getDataProfile() {\n        return apiAxios({\n            method: 'get',\n            url: '/users',\n        })\n    },\n    //update profile\n    updateProfile(data) {\n        return apiAxios({\n            method: 'put',\n            url: '/users/change-profile',\n            data: data\n        })\n    },\n    //update password\n    changePassword(data) {\n        return apiAxios({\n            method: 'put',\n            url: '/users/change-password',\n            data: data\n        })\n    },\n    //get data order  \n    getDataOrder() {\n        return apiAxios({\n            method: 'get',\n            url: '/reports',\n        })\n    },\n    // get data cho cot thong ke doanh thu theo ngay\n    getTotalValue() {\n        return apiAxios({\n            method: 'get',\n            url: '/reports/get-order',\n        })\n    },\n    //get data cho cot thong ke doanh thu theo thang\n    getTotalValueMonth() {\n        return apiAxios({\n            method: 'get',\n            url: '/reports/get-order-month',\n        })\n    },\n    // crud customer\n    getDataCustomer() {\n        return apiAxios({\n            method: 'get',\n            url: '/customers',\n        })\n    },\n    addNewCustomer(data) {\n        return apiAxios({\n            method: 'post',\n            url: '/customers',\n            data: data\n        })\n    },\n    deleteCustomer(id) {\n        return apiAxios({\n            method: 'delete',\n            url: '/customers/' + id,\n        })\n    },\n    // crud auth user\n    getAuthUser() {\n        return apiAxios({\n            method: 'get',\n            url: '/auth/me'\n        })\n    },\n    login(data) {\n        return apiAxios({\n            method: 'post',\n            url: 'auth/login',\n            data: data\n        })\n    },\n    // crud product\n    getDataProducts(params) {\n        return apiAxios({\n            method: 'get',\n            url: '/products',\n            params: params\n        })\n    },\n    addProduct(data) {\n        return apiAxios({\n            method: 'post',\n            url: '/products',\n            data: data\n        })\n    },\n    deleteProduct(id) {\n        return apiAxios({\n            method: 'delete',\n            url: '/products/' + id,\n        })\n    },\n    updateProduct(id, data) {\n        return apiAxios({\n            method: 'post',\n            url: '/products/' + id,\n            data: data\n        })\n    },\n    getDataCategories() {\n        return apiAxios({\n            method: 'get',\n            url: '/categories/all',\n        })\n    },\n    // crud categories\n    getDataCategoriesPaginate(params) {\n        return apiAxios({\n            method: 'get',\n            url: '/categories',\n            params: params\n        })\n    },\n    addNewCategory(data) {\n        return apiAxios({\n            method: 'post',\n            url: '/categories',\n            data: data\n        })\n    },\n    upadateCategory(id, data) {\n        return apiAxios({\n            method: 'put',\n            url: '/categories/' + id,\n            data: data\n        })\n    },\n    deleteCategory(id) {\n        return apiAxios({\n            method: 'delete',\n            url: '/categories/' + id,\n        })\n    },\n\n    // carts\n    getDataCarts() {\n        return apiAxios({\n            method: 'get',\n            url: '/carts',\n        })\n    },\n    addCart(id) {\n        return apiAxios({\n            method: 'post',\n            url: '/carts/products/' + id\n        })\n    },\n    addUpdateCart(id, data) {\n        return apiAxios({\n            method: 'put',\n            url: '/carts/products/' + id,\n            data: data\n        })\n    },\n    deleteCart(id) {\n        return apiAxios({\n            method: 'delete',\n            url: '/carts/' + id,\n        })\n    },\n    storeOrder(data) {\n        return apiAxios({\n            method: 'post',\n            url: '/carts/store-order',\n            data: data\n        })\n    },\n    getDataOrders(params) {\n        return apiAxios({\n            method: 'get',\n            url: '/orders',\n            params: params\n        })\n    },\n    changeStatusOrders(id, data) {\n        return apiAxios({\n            method: 'put',\n            url: '/orders/change-status/' + id,\n            data: data\n        })\n    },\n    //File\n    importProductExcel(data) {\n        return apiAxios({\n            method: 'post',\n            url: '/products/import',\n            data: data\n        })\n    },\n}"]}]}