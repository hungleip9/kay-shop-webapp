4BAuBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAzBA;AA0BA,IAAA,SA1BA,uBA0BA;AACA,UAAA,MAAA,CAAA,YAAA,CAAA,IAAA,KAAA,OAAA,EAAA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAAA;AACA;AACA,KA9BA;AA+BA,IAAA,YA/BA,0BA+BA;AACA,UAAA,MAAA,CAAA,YAAA,CAAA,IAAA,KAAA,YAAA,EAAA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAAA;AACA;AACA,KAnCA;AAoCA,IAAA,eApCA,6BAoCA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA,QAAA;AACA,KAtCA;AAuCA,IAAA,QAvCA,sBAuCA;AAAA;;AACA,MAAA,GAAA,CAAA,YAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,CAAA,CAAA,GAAA,CAAA,QAAA,EAAA,WAAA,EAAA,MAAA;AACA,OAFA;AAGA;AA3CA,IALA;AAmDA,EAAA,OAnDA,qBAmDA;AAAA;;AACA,SAAA,QAAA;AACA,SAAA,eAAA;AACA,IAAA,GAAA,CAAA,WAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,CAAA,QAAA,CAAA,IAAA;AACA;AACA,KAJA;AAKA;AA3DA,CAAA","sourcesContent":["<script src=\"../store/modules/home/index.js\"></script>\n<template>\n  <el-container>\n    <el-aside class=\"sideBar\">\n      <div class=\"logoWrap\">\n        <img src=\"../assets/images/logo_dark.png\" alt=\"\" />\n      </div>\n      <el-menu\n        default-active=\"1\"\n        text-color=\"#253036\"\n        active-text-color=\"#27AE60\"\n      >\n        <el-menu-item index=\"1\" @click=\"dashboard()\">\n          <i class=\"fa fa-th-large\" aria-hidden=\"true\"></i>\n          <span> Tổng quan</span>\n        </el-menu-item>\n        <el-menu-item index=\"2\" @click=\"goToCagories\">\n          <i class=\"fa fa-bars\" aria-hidden=\"true\"></i>\n          <span> Quản lý nhóm sản phẩm</span>\n        </el-menu-item>\n        <el-menu-item index=\"3\" @click=\"goToProducts\">\n          <i class=\"fa fa-archive\" aria-hidden=\"true\"></i>\n          <span> Quản lý sản phẩm</span>\n        </el-menu-item>\n        <el-menu-item index=\"4\" @click=\"customerManagement()\">\n          <i class=\"fa fa-user\"></i>\n          <span> Quản lý khách hàng</span>\n        </el-menu-item>\n        <el-menu-item index=\"5\" @click=\"orderManagement\">\n          <i class=\"el-icon-s-order icon-order\"></i>\n          <span> Quản lý đơn hàng</span>\n        </el-menu-item>\n      </el-menu>\n    </el-aside>\n\n    <el-container>\n      <el-header class=\"header\">\n        <div class=\"headerLeftWrap\">\n          {{ pageTitle }}\n        </div>\n        <div class=\"headerRightWrap\">\n          <el-button\n            @click=\"goToCarts\"\n            class=\"cart-button\"\n            plain\n          >\n            <i class=\"fa fa-shopping-cart\"></i> Bán hàng</el-button\n          >\n          <el-dropdown>\n            <el-avatar\n              size=\"medium\"\n              shape=\"circle\"\n              src=\"https://saoexpress.vn/wp-content/uploads/2019/07/hot-girl-nguyen-thanh-thuy-Saoexpress-1.jpg\"\n            />\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item @click.native=\"Profile\"\n                >Thông tin tài khoản</el-dropdown-item\n              >\n              <el-dropdown-item @click.native=\"changePassword\"\n                >Đổi mật khẩu</el-dropdown-item\n              >\n              <el-dropdown-item divided @click.native=\"logout\">\n                Đăng xuất\n              </el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </el-header>\n\n      <el-main>\n        <router-view />\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script>\nimport { mapMutations, mapActions, mapState } from \"vuex\";\nimport api from \"../api\";\nimport _ from 'lodash'\nimport router from \"@/router\";\n\nexport default {\n  name: \"AdminLayout\",\n  computed: {\n    ...mapState([\"pageTitle\",\"cartSum\"]),\n  },\n  methods: {\n    ...mapMutations(\"auth\", [\"updateAuthUser\"]),\n    ...mapMutations([\"updateTitle\",\"sumCart\"]),\n    ...mapActions(\"auth\", [\"logout\"]),\n    changePageTitle() {\n      this.updateTitle(\"Tổng quát\");\n    },\n    dashboard() {\n      this.$router.push({name:'Home'})\n    },\n    customerManagement() {\n      if (router.currentRoute.name !== \"Customer\") {\n        return router.push({ name: \"Customer\" });\n      }\n    },\n    goToProducts() {\n      if (router.currentRoute.name !== \"Products\") {\n        return router.push({ name: \"Products\" });\n      }\n    },\n    Profile() {\n      this.$router.push({ name: \"Profile\" });\n    },\n    changePassword() {\n      this.$router.push({ name: \"ChangePassword\" });\n    },\n    goToCarts() {\n      if (router.currentRoute.name !== \"Carts\") {\n        return router.push({ name: \"Carts\" });\n      }\n    },\n    goToCagories() {\n      if (router.currentRoute.name !== \"Categories\") {\n        return router.push({ name: \"Categories\" });\n      }\n    },\n    orderManagement() {\n      this.$router.replace(\"/order\");\n    },\n    getCarts() {\n      api.getDataCarts().then((response) => {\n          this.sumCart(_.get(response,'data.data').length)\n      })\n    },\n  },\n\n  mounted() {\n    this.getCarts()\n    this.changePageTitle();\n    api.getAuthUser().then((response) => {\n      if (response) {\n        this.updateAuthUser(response.data);\n      }\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../assets/styles/variables\";\n\n.sideBar {\n  width: 250px !important;\n  height: 100vh;\n  box-shadow: inset -10px 0 10px -5px rgba(0, 0, 0, 0.1);\n  .el-menu-item {\n  }\n  .el-menu-item:hover {\n    background-color: $colorBrand!important;\n    color: $colorBrandText!important;\n    i{\n      color: $colorBrandText!important;\n    }\n  }\n  .logoWrap {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    margin-bottom: 12px;\n\n    img {\n      width: 150px;\n      height: 65px;\n    }\n  }\n\n  .el-menu {\n    border: none;\n    box-shadow: inset -10px 0 10px -5px rgba(0, 0, 0, 0.1);\n  }\n  .is-active {\n    background-color: $colorBrand !important;\n  }\n}\n\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  border-bottom: 1px solid $colorStroke;\n  height: 60px;\n  width: 100%;\n  background: $colorHeaderBg;\n  padding: 0 24px;\n\n  .headerLeftWrap {\n    display: flex;\n    align-items: center;\n    font-size: 24px;\n    font-weight: bold;\n  }\n\n  .headerRightWrap {\n    display: flex;\n    align-items: center;\n    .cart-button {\n      margin-right: 40px;\n    }\n    .notification {\n      margin-right: 40px;\n      font-size: 24px;\n    }\n  }\n}\n\n.el-main {\n  overflow-x: hidden;\n  overflow-y: auto;\n  max-height: 93vh;\n  padding: 20px;\n  background-color: $colorMainBg;\n}\n\n.el-main::-webkit-scrollbar {\n  width: 10px;\n  height: 10px;\n}\n\n.el-main::-webkit-scrollbar-thumb {\n  background-color: #c0c4cc;\n  border-radius: 8px;\n}\n.icon-order {\n  font-size: 17px !important;\n  margin-right: 0px !important;\n  width: 14px !important;\n  text-align: left;\n}\n</style>\n"],"sourceRoot":"src/layouts"}]}