     <div class=\"head-product\">\n        <div class=\"search\">\n          <el-input\n            class=\"search-input\"\n            placeholder=\"Nhập tên sản phẩm để tìm kiếm\"\n            v-model=\"search\"\n            @keydown.enter.native=\"handleSearch\"\n          ></el-input>\n          <el-button @click=\"handleSearch\">Tìm kiếm</el-button>\n        </div>\n        <div>\n         \n\n          <el-button\n            class=\"show-card\"\n            type=\"primary\"\n            @click=\"dialogAddProduct = true\"\n            ><i class=\"fa fa-plus\"></i> Thêm mới</el-button>\n          <el-button\n          class=\"show-card\"\n          type=\"primary\"\n          @click=\"openDialogExcel\"\n\n          ><i class=\"fa fa-upload\"></i> Chọn tệp</el-button>\n        </div>\n      </div>\n    </el-card>\n    <el-card class=\"table-product\">\n      <el-table :data=\"products\" stripe style=\"width: 100%\">\n        <el-table-column prop=\"image\" label=\"Ảnh sản phẩm\" width=\"130px\">\n          <template slot-scope=\"scope\">\n            <img\n              class=\"product-image\"\n              v-if=\"scope.row.image\"\n              :src=\"baseUrl + scope.row.image\"\n              alt=\"\"\n            />\n            <img\n              class=\"product-image\"\n              v-else\n              src=\"../assets/images/product.jpg\"\n              alt=\"\"\n            />\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"sku\" label=\"SKU\"> </el-table-column>\n        <el-table-column prop=\"name\" label=\"Tên sản phẩm\"> </el-table-column>\n        <el-table-column\n          prop=\"quantity_in_stock\"\n          label=\"Tồn kho\"\n          align=\"center\"\n        >\n        </el-table-column>\n        <el-table-column prop=\"sale_price\" label=\"Giá bán\">\n          <template slot-scope=\"scope\">\n            {{ Number(scope.row.sale_price).toLocaleString(\"vi-VN\") }} VNĐ\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"original_price\" label=\"Giá nhập\">\n          <template slot-scope=\"scope\">\n            {{ Number(scope.row.original_price).toLocaleString(\"vi-VN\") }} VNĐ\n          </template>\n        </el-table-column>\n        <el-table-column label=\"Hành động\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-popover placement=\"bottom\" width=\"100\" trigger=\"click\">\n              <div class=\"product-action\" @click=\"editProduct(scope.row)\">\n                <i class=\"fa fa-pencil\"></i> Chỉnh sửa\n              </div>\n              <div\n                class=\"product-action\"\n                @click=\"handleDeleteProduct(scope.row._id)\"\n              >\n                <i class=\"fa fa-trash\"></i> Xóa\n              </div>\n              <div class=\"btn-edit\" slot=\"reference\">\n                <i class=\"fa fa-ellipsis-v\"></i>\n              </div>\n            </el-popover>\n          </template>\n        </el-table-column>\n      </el-table>\n      <div class=\"pagination\">\n        <el-pagination\n          background\n          layout=\"prev, pager, next\"\n          :current-page=\"currentPage\"\n          :total=\"total\"\n          @current-change=\"handleChangePage\"\n        >\n        </el-pagination>\n      </div>\n    </el-card>\n    <el-dialog\n      class=\"dialog-add-product\"\n      width=\"900px\"\n      title=\"Thêm sản phẩm\"\n      :visible.sync=\"dialogAddProduct\"\n    >\n      <el-row>\n        <el-col :span=\"12\">\n          <div class=\"add-product-image-title\">Ảnh sản phẩm</div>\n          <div class=\"add-product-image\">\n            <div class=\"product-image\">\n              <label\n                for=\"product-input\"\n                @mouseover=\"hoverImage\"\n                @mouseleave=\"hoverLeaveImage\"\n              >\n                <div class=\"productHover\" ref=\"productHover\">\n                  <i class=\"fa fa-pencil\"></i>\n                </div>\n                <img\n                  v-if=\"newProductImageUrl\"\n                  :src=\"newProductImageUrl\"\n                  alt=\"\"\n                />\n                <img v-else src=\"../assets/images/product.jpg\" alt=\"\" />\n              </label>\n              <input\n                ref=\"productImage\"\n                type=\"file\"\n                accept=\"image/*\"\n                id=\"product-input\"\n                @change=\"handleChangeImage\"\n              />\n              <center>\n                <div\n                  v-if=\"newProductImageUrl\"\n                  class=\"delete-product-image\"\n                  @click=\"deleteImage\"\n                >\n                  Xóa ảnh\n                </div>\n              </center>\n            </div>\n          </div>\n        </el-col>\n        <el-col :span=\"12\">\n          <el-form\n            class=\"form-product\"\n            :model=\"form\"\n            :rules=\"rules\"\n            ref=\"formAddProduct\"\n            label-position=\"top\"\n            hide-required-asterisk\n            label-width=\"130px\"\n            size=\"small\"\n          >\n            <el-row>\n              <el-col :span=\"24\" class=\"margin-input\">\n                <el-form-item prop=\"name\">\n                  <label class=\"label-product\"\n                    >Tên sản phẩm <span class=\"required\"> *</span></label\n                  >\n                  <el-input\n                    size=\"medium\"\n                    v-model=\"form.name\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item prop=\"category_ids\">\n                  <label class=\"label-product\"\n                    >Nhóm sản phẩm <span class=\"required\"> *</span></label\n                  >\n                  <el-select\n                    class=\"select-categories\"\n                    v-model=\"form.category_ids\"\n                    placeholder=\"\"\n                    size=\"medium\"\n                    multiple\n                    filterable\n                  >\n                    <el-option\n                      v-for=\"category in categories\"\n                      :key=\"category._id\"\n                      :label=\"category.name\"\n                      :value=\"category._id\"\n                      >{{ category.name }}</el-option\n                    >\n                  </el-select>\n                </el-form-item>\n                <el-form-item>\n                  <el-row :gutter=\"20\">\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"original_price\">\n                        <label class=\"label-product\"\n                          >Giá nhập (VNĐ)\n                          <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.original_price\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                          min=\"0\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\" :gutter=\"40\">\n                      <el-form-item prop=\"sale_price\">\n                        <label class=\"label-product\"\n                          >Giá bán (VNĐ) <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.sale_price\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                          min=\"0\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"weight\">\n                        <label class=\"label-product\">Cân nặng (kg)</label>\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.weight\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                          min=\"0\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"quantity_in_stock\">\n                        <label class=\"label-product\"\n                          >Số lượng <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          type=\"number\"\n                          size=\"medium\"\n                          v-model=\"form.quantity_in_stock\"\n                          autocomplete=\"off\"\n                          min=\"0\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                  </el-row>\n                </el-form-item>\n                <el-form-item prop=\"description\">\n                  <label class=\"label-product\">Mô tả</label>\n                  <el-input\n                    size=\"medium\"\n                    v-model=\"form.description\"\n                    :autosize=\"{ minRows: 4, maxRows: 6 }\"\n                    autocomplete=\"off\"\n                    type=\"textarea\"\n                  ></el-input>\n                </el-form-item>\n              </el-col>\n            </el-row>\n          </el-form>\n        </el-col>\n      </el-row>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogAddProduct = false\">Hủy</el-button>\n        <el-button\n          type=\"primary\"\n          :plain=\"true\"\n          @click=\"storeProduct('formAddProduct')\"\n          >Lưu</el-button\n        >\n      </span>\n    </el-dialog>\n    <el-dialog\n      class=\"dialog-add-product\"\n      width=\"900px\"\n      title=\"Chỉnh sửa sản phẩm\"\n      :visible.sync=\"dialogEditProduct\"\n    >\n      <el-row>\n        <el-col :span=\"12\">\n          <div class=\"add-product-image-title\">Ảnh sản phẩm</div>\n          <div class=\"add-product-image\">\n            <div class=\"product-image\">\n              <label\n                for=\"product-edit-input\"\n                @mouseover=\"hoverImage\"\n                @mouseleave=\"hoverLeaveImage\"\n              >\n                <div class=\"productHover\" ref=\"productEditHover\">\n                  <i class=\"el-icon-edit\"></i>\n                </div>\n                <img\n                  v-if=\"newProductImageUrl\"\n                  :src=\"newProductImageUrl\"\n                  alt=\"\"\n                />\n                <img v-else src=\"../assets/images/product.jpg\" alt=\"\" />\n              </label>\n              <input\n                ref=\"productImage\"\n                type=\"file\"\n                accept=\"image/*\"\n                id=\"product-edit-input\"\n                @change=\"handleChangeImage\"\n              />\n            </div>\n          </div>\n        </el-col>\n        <el-col :span=\"12\">\n          <el-form\n            class=\"form-product\"\n            :model=\"form\"\n            :rules=\"rules\"\n            ref=\"formEditProduct\"\n            label-position=\"top\"\n            hide-required-asterisk\n            label-width=\"130px\"\n            size=\"small\"\n          >\n            <el-row>\n              <el-col :span=\"24\" class=\"margin-input\">\n                <el-form-item prop=\"name\">\n                  <label class=\"label-product\"\n                    >Tên sản phẩm <span class=\"required\"> *</span></label\n                  >\n                  <el-input\n                    size=\"medium\"\n                    v-model=\"form.name\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item prop=\"sku\">\n                  <label class=\"label-product\"\n                    >Mã sản phẩm <span class=\"required\"> *</span></label\n                  >\n                  <el-input\n                    size=\"medium\"\n                    v-model=\"form.sku\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item prop=\"category_ids\">\n                  <label class=\"label-product\"\n                    >Nhóm sản phẩm <span class=\"required\"> *</span></label\n                  >\n                  <el-select\n                    class=\"select-categories\"\n                    v-model=\"form.category_ids\"\n                    placeholder=\"\"\n                    size=\"medium\"\n                    multiple\n                    filterable\n                  >\n                    <el-option\n                      v-for=\"category in categories\"\n                      :key=\"category._id\"\n                      :label=\"category.name\"\n                      :value=\"category._id\"\n                      >{{ category.name }}</el-option\n                    >\n                  </el-select>\n                </el-form-item>\n                <el-form-item>\n                  <el-row :gutter=\"20\">\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"original_price\">\n                        <label class=\"label-product\"\n                          >Giá nhập (VNĐ)\n                          <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.original_price\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\" :gutter=\"40\">\n                      <el-form-item prop=\"sale_price\">\n                        <label class=\"label-product\"\n                          >Giá bán (VNĐ) <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.sale_price\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"weight\">\n                        <label class=\"label-product\">Cân nặng (kg)</label>\n                        <el-input\n                          size=\"medium\"\n                          v-model=\"form.weight\"\n                          autocomplete=\"off\"\n                          type=\"number\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\">\n                      <el-form-item prop=\"quantity_in_stock\">\n                        <label class=\"label-product\"\n                          >Số lượng <span class=\"required\"> *</span></label\n                        >\n                        <el-input\n                          type=\"number\"\n                          size=\"medium\"\n                          v-model=\"form.quantity_in_stock\"\n                          autocomplete=\"off\"\n                        ></el-input>\n                      </el-form-item>\n                    </el-col>\n                  </el-row>\n                </el-form-item>\n                <el-form-item prop=\"description\">\n                  <label class=\"label-product\">Mô tả</label>\n                  <el-input\n                    size=\"medium\"\n                    v-model=\"form.description\"\n                    :autosize=\"{ minRows: 4, maxRows: 6 }\"\n                    autocomplete=\"off\"\n                    type=\"textarea\"\n                  ></el-input>\n                </el-form-item>\n              </el-col>\n            </el-row>\n          </el-form>\n        </el-col>\n      </el-row>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogEditProduct = false\">Hủy</el-button>\n        <el-button\n          type=\"primary\"\n          :plain=\"true\"\n          @click=\"handleUpdateProduct('formEditProduct')\"\n          >Lưu</el-button\n        >\n      </span>\n    </el-dialog>\n\n    <el-dialog width=\"50%\" title=\"Nhập Excel\" :visible.sync=\"dialogExcel\" @close=\"closeImport\">\n     \n      <div class=\"upload-excel\">\n        <i class=\"el-icon-upload\"></i>\n        <div class=\"el-upload__text\" v-html=\"uploadText\"></div>\n        <input type=\"file\" @change=\"changeFile\">\n      </div>\n      \n      <!-- <div v-if=\"this.errorImageLink !== '' \" class=\"error\">\n        <span> {{ errorImageLink }} </span>\n      </div> -->\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeDailogExcel\">Đóng</el-button>\n        <el-button type=\"primary\" @click=\"importExcel\"><i class=\"el-icon-upload2\"></i> Tải lên</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport api from \"../api\";\nimport { mapMutations } from \"vuex\";\nexport default {\n  name: \"Products\",\n  data() {\n    return {\n      uploadText: \"\",\n      file: \"\",\n      search: \"\",\n      products: [],\n      dialogAddProduct: false,\n      dialogEditProduct: false,\n      dialogExcel: false,\n      form: {\n        name: \"\",\n        sku: \"\",\n        image: \"\",\n        description: \"\",\n        category_ids: [],\n        weight: \"\",\n        sale_price: \"\",\n        original_price: \"\",\n        quantity_in_stock: \"\",\n      },\n      rules: {\n        name: [\n          {\n            required: true,\n            message: \"Tên sản phẩm không được bỏ trống!\",\n            trigger: \"change\",\n          },\n        ],\n        sku: [\n          {\n            required: true,\n            message: \"Mã sản phẩm không được bỏ trống!\",\n            trigger: \"change\",\n          },\n        ],\n        category_ids: [\n          {\n            required: true,\n            message: \"Danh mục không được bỏ trống!\",\n            trigger: \"change\",\n          },\n        ],\n        sale_price: [\n          {\n            required: true,\n            message: \"Giá bán không được bỏ trống!\",\n            trigger: \"change\",\n          },\n        ],\n        original_price: [\n          {\n            required: true,\n            message: \"Giá nhập không được bỏ trống!\",\n            trigger: \"change\",\n          },\n        ],\n        quantity_in_stock: [\n          {\n            required: true,\n            message: \"Nhập số lượng sản phẩm\",\n            trigger: \"change\",\n          },\n        ],\n      },\n      newProductImageUrl: \"\",\n      newProductUploadImage: \"\",\n      total: 0,\n      currentPage: 1,\n      baseUrl: process.env.VUE_APP_BASE_URL + \"/storage/\",\n      // baseUrl: 'http://127.0.0.1:8000/storage/',\n      productEditId: \"\",\n      categories: [],\n    };\n  },\n  methods: {\n    ...mapMutations([\"updateTitle\"]),\n    changePageTitle() {\n      this.updateTitle(\"Danh sách sản phẩm\");\n    },\n    openDialogExcel(){\n      this.dialogExcel = true\n      this.uploadText = \"Thả tệp vào đây hoặc <em style='color: #409EFF; font-style: normal;'>bấm đê tải tệp lên</em>\"\n    },\n    closeDailogExcel() {\n      this.dialogExcel = false\n    },\n    //upload file excel\n    importExcel() {\n      const frmData = new FormData()\n        frmData.append('file', this.file)\n        api.importProductExcel(frmData).then(() => {\n          this.dialogExcel = false\n          this.$message({\n            type: 'success',\n            message: 'Nhập thành công',\n            center:true\n          })\n        })\n        this.getData()\n    },\n     changeFile(e) {\n      this.file = e.target.files[0]\n      this.uploadText = 'Đã chọn 1 file: ' + this.file.name\n    },\n    closeImport() {\n      this.file = null\n\n    },\n    hoverImage() {\n      this.$refs.productHover.style.display = \"flex\";\n    },\n    hoverLeaveImage() {\n      this.$refs.productHover.style.display = \"none\";\n    },\n    handleChangeImage(e) {\n      if (e.target.files.length) {\n        this.newProductUploadImage = e.target.files[0];\n        this.newProductImageUrl = URL.createObjectURL(e.target.files[0]);\n      }\n    },\n    deleteImage() {\n      this.newProductUploadImage = \"\";\n      this.newProductImageUrl = \"\";\n    },\n    storeProduct(formAddProduct) {\n      this.$refs[formAddProduct].validate((valid) => {\n        if (valid) {\n          const frmData = new FormData();\n          frmData.append(\"name\", this.form.name);\n          frmData.append(\"category_ids\", this.form.category_ids);\n          frmData.append(\"original_price\", this.form.original_price);\n          frmData.append(\"sale_price\", this.form.sale_price);\n          if (this.form.weight) {\n            frmData.append(\"weight\", this.form.weight);\n          } else {\n            frmData.append(\"weight\", 0);\n          }\n          frmData.append(\"quantity_in_stock\", this.form.quantity_in_stock);\n          frmData.append(\"description\", this.form.description);\n          if (this.newProductUploadImage) {\n            frmData.append(\"image\", this.newProductUploadImage);\n          }\n          api\n            .addProduct(frmData)\n            .then(() => {\n              this.newProductUploadImage = \"\";\n              this.newProductImageUrl = \"\";\n              this.$message({\n                message: \"Thêm mới thành công!\",\n                type: \"success\",\n              });\n              this.dialogAddProduct = false;\n              this.getData();\n            })\n            .catch(() => {\n              this.$message({\n                message: \"Thêm mới thất bại!\",\n                type: \"error\",\n              });\n            });\n        }\n      });\n    },\n    editProduct(product) {\n      this.dialogEditProduct = true;\n      this.productEditId = product._id;\n      this.form.name = product.name;\n      this.form.sku = product.sku;\n      this.form.category_ids = product.category_ids;\n      this.form.original_price = product.original_price;\n      this.form.sale_price = product.sale_price;\n      this.form.weight = product.weight;\n      this.form.quantity_in_stock = product.quantity_in_stock;\n      this.form.description = product.description;\n      if (product.image) {\n        this.newProductImageUrl = this.baseUrl + product.image;\n      }\n    },\n    handleUpdateProduct(formEditProduct) {\n      this.$refs[formEditProduct].validate((valid) => {\n        if (valid) {\n          const frmData = new FormData();\n          frmData.append(\"name\", this.form.name);\n          frmData.append(\"sku\", this.form.sku);\n          frmData.append(\"category_ids\", this.form.category_ids);\n          frmData.append(\"original_price\", this.form.original_price);\n          frmData.append(\"sale_price\", this.form.sale_price);\n          if (this.form.weight) {\n            frmData.append(\"weight\", this.form.weight);\n          } else {\n            frmData.append(\"weight\", 0);\n          }\n          frmData.append(\"quantity_in_stock\", this.form.quantity_in_stock);\n          frmData.append(\"description\", this.form.description);\n          if (this.newProductUploadImage) {\n            frmData.append(\"image\", this.newProductUploadImage);\n          }\n          api\n            .updateProduct(this.productEditId, frmData)\n            .then(() => {\n              this.newProductUploadImage = \"\";\n              this.newProductImageUrl = \"\";\n              this.$message({\n                message: \"Cập nhật thành công.\",\n                type: \"success\",\n                center:true\n              });\n              this.dialogEditProduct = false;\n              let params = {\n                page: this.currentPage,\n                q: this.search,\n              };\n              api.getDataProducts(params).then((response) => {\n                this.products = response.data.data.data;\n              });\n            })\n            .catch(() => {\n              this.$message({\n                message: \"Cập nhật thất bại!\",\n                type: \"error\",\n                center:true\n              });\n            });\n        }\n      });\n    },\n    handleDeleteProduct(id) {\n      this.$confirm(\"Bạn có chắc chắn muốn xóa sản phẩm này?\", \"Cảnh báo\", {\n        confirmButtonText: \"Xóa\",\n        cancelButtonText: \"Hủy\",\n        type: \"warning\",\n      })\n        .then(() => {\n          api\n            .deleteProduct(id)\n            .then(() => {\n              this.$message({\n                message: \"Xóa sản phẩm thành công!\",\n                type: \"success\",\n                center:true\n              });\n              let params = {\n                page: this.currentPage,\n              };\n              api.getDataProducts(params).then((response) => {\n                this.products = response.data.data.data;\n              });\n            })\n            .catch(() => {\n              this.$message({\n                message: \"Xóa sản phẩm thất bại!\",\n                type: \"error\",\n                center:true\n              });\n            });\n        })\n        .catch(() => {});\n    },\n    handleChangePage(page) {\n      this.currentPage = page;\n      le