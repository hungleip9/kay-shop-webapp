ning'\n            })\n        },\n        isValidData () {\n        let error = false;\n        this.errorName = '';\n        this.errorTotalValue = '';\n        this.errorStatus = '';\n        if (this.name.length < 4) {\n          error = true;\n          this.errorName = 'Tên phải lớn hơn 4 ký tự';\n        }\n        if (this.name.length === 0) {\n          error = true;\n          this.errorPassword = 'Tên không được để trống';\n        }\n        if (this.total_value.length < 9) {\n          error = true;\n          this.errorTotalValue = 'Tổng tiền phải lớn hơn 9 ký tự';\n        }\n        if (this.total_value.length > 10) {\n          error = true;\n          this.errorTotalValue = 'Tổng tiền phải nhỏ hơn 10 ký tự';\n        }\n        if (this.total_value.length === 0) {\n          error = true;\n          this.errorTotalValue = 'Tổng tiền không được để trống';\n        }\n        if (this.status.length === 0) {\n          error = true;\n          this.errorStatus = 'Trạng thái không được để trống';\n        }\n\n        return !error\n      }\n    }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n@import \"../assets/styles/variables\";\n\n.dashboard {\n    label {\n        font-weight: bold;\n        margin-bottom: 10px;\n    }\n}\n//el card\n.text {\n    font-size: 14px;\n  }\n\n  .item {\n    margin-bottom: 18px;\n  }\n\n  .clearfix:before,\n  .clearfix:after {\n    display: table;\n    content: \"\";\n  }\n  .clearfix:after {\n    clear: both\n  }\n  .box-card {\n    width: 100%;\n    height: 100%;\n    .title {\n        font-weight: bold;\n        font-size: 25px;\n        line-height: 40px;\n    }\n    .btn-add-user {\n        height: 40px;\n        width: 150px;\n        border-radius: 21px!important;\n        color: white;\n        font-weight: bold;\n        background-color: $colorBlue;\n    }\n    .box-search {\n        label {\n            margin-bottom: 10px!important;\n   