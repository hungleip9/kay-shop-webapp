iv v-if=\"this.errorTotalValue !== '' \" class=\"error\">\n                                <span> {{ errorTotalValue }} </span>\n                            </div>\n                            <span>Trạng thái</span><span class=\"star\">*</span><br>\n                            <el-select class=\"select-add\" v-model=\"status\">\n                                <el-option value=\"\">Đang chờ xử lý</el-option>\n                                <el-option value=\"\">Đã thanh toán</el-option>\n                                <el-option value=\"\">Đang giao hàng</el-option>\n                                <el-option value=\"\">Huỷ đơn</el-option>\n                            </el-select>\n                            <div v-if=\"this.errorStatus !== '' \" class=\"error\">\n                                <span> {{ errorStatus }} </span>\n                            </div>\n                        </el-col>\n                    </el-row>\n                    <span slot=\"footer\" class=\"dialog-footer\">\n                        <el-button @click=\"dialogEditOrder = false\">Thoát</el-button>\n                        <el-button type=\"primary\" @click=\"editOrder\">Chỉnh sửa</el-button>\n                    </span>\n                </el-dialog>\n            </div>\n            <div class=\"box-search\">\n                <el-row :gutter=\"80\">\n                    <el-col :span=\"6\" class=\"box-search-mini\">\n                        <label for=\"\">Tên khách hàng</label><br>\n                        <el-input class=\"search-name\" type=\"text\"></el-input>\n                    </el-col>\n                    <el-col :span=\"6\" class=\"box-search-mini\">\n                        <label for=\"\">Mã hoá đơn</label><br>\n                        <el-input class=\"search-code\" type=\"text\"></el-input>\n                    </el-col>\n                    <el-col :span=\"6\" class=\"box-search-mini\">\n                        <label for=\"\">Trạng thái</label><br>\n            