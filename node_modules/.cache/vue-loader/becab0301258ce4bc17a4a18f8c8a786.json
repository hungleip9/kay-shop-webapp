plate>\n  <div class=\"dashboard\">\n        <el-card class=\"box-card\">\n            <div slot=\"header\" class=\"clearfix\">\n                <span class=\"title\">Quản lý khách hàng</span>\n                <el-button style=\"float: right; padding: 3px 0\" type=\"text\" class=\"btn-add-user\" @click=\"dialogAddCustomer = true\">Thêm khách hàng</el-button>\n                <!-- Them moi khach hang -->\n                <el-dialog\n                    title=\"Thêm khách hàng\"\n                    :visible.sync=\"dialogAddCustomer\"\n                    width=\"45%\"\n                    :before-close=\"closeAddCustomer\">\n                    <el-row :gutter=\"80\">\n                        <el-col :span=\"12\" class=\"box-add-customer\">\n                            <div class=\"label-add\">\n                                <span>Tên khách hàng</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"name\"></el-input><br>\n                            <div v-if=\"this.errorName !== '' \" class=\"error\">\n                                <span> {{ errorName }} </span>\n                            </div>\n                            <div class=\"label-add\">\n                                <span>Email</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"email\"></el-input><br>\n                        </el-col>\n                        <el-col :span=\"12\" class=\"box-add-customer\">\n                            <div class=\"label-add\">\n                                <span>Di động</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"phone\"></el-input><br>\n                            <div v-if=\"this.errorPhone !== '' \" class=\"error\">\n                                <span> {{ errorPhone }} </span>\n                            </div>\n                            <div class=\"label-add\">\n                                <span class=\"label-add\">Địa chỉ</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"address\"></el-input><br>\n                            <div v-if=\"this.errorAddRess !== '' \" class=\"error\">\n                                <span> {{ errorAddRess }} </span>\n                            </div>\n                        </el-col>\n                    </el-row>\n                    <span slot=\"footer\" class=\"dialog-footer\">\n                        <el-button @click=\"closeAddCustomerBtn\">Thoát</el-button>\n                        <el-button type=\"primary\" @click=\"addNewCustomer\">Thêm</el-button>\n                    </span>\n                </el-dialog>\n                <!-- chinh sua khach hang -->\n                <el-dialog\n                    title=\"Chỉnh sửa khách hàng\"\n                    :visible.sync=\"dialogEditCustomer\"\n                    width=\"45%\"\n                    :before-close=\"closeEditCustomer\">\n                    <el-row :gutter=\"80\">\n                        <el-col :span=\"12\" class=\"box-add-customer\">\n                            <div class=\"label-add\">\n                                <span>Tên khách hàng</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"name\"></el-input><br>\n                            <div v-if=\"this.errorName !== '' \" class=\"error\">\n                                <span> {{ errorName }} </span>\n                            </div>\n                            <div class=\"label-add\">\n                                <span>Email</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"email\"></el-input><br>\n                        </el-col>\n                        <el-col :span=\"12\" class=\"box-add-customer\">\n                            <div class=\"label-add\">\n                                <span>Di động</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"phone\"></el-input><br>\n                            <div v-if=\"this.errorPhone !== '' \" class=\"error\">\n                                <span> {{ errorPhone }} </span>\n                            </div>\n                            <div class=\"label-add\">\n                                <span class=\"label-add\">Địa chỉ</span><span class=\"star\">*</span>\n                            </div>\n                            <el-input class=\"input-add\" v-model=\"address\"></el-input><br>\n                            <div v-if=\"this.errorAddRess !== '' \" class=\"error\">\n                                <span> {{ errorAddRess }} </span>\n                            </div>\n                        </el-col>\n                    </el-row>\n                    <span slot=\"footer\" class=\"dialog-footer\">\n                        <el-button @click=\"closeEditCustomerBtn\">Thoát</el-button>\n                        <el-button type=\"primary\" @click=\"editCustomer\">Chỉnh sửa</el-button>\n                    </span>\n                </el-dialog>\n            </div>\n            <div class=\"box-search\">\n                <el-row :gutter=\"80\">\n                    <el-col :span=\"6\" class=\"box-search-mini\">\n                        <label for=\"\">Tên khách hàng</label><br>\n                        <el-input class=\"search-name\" prefix-icon=\"el-icon-search\"></el-input>\n                    </el-col>\n                    <el-col :span=\"6\" class=\"box-search-mini\">\n                        <label for=\"\">Mã khách hàng</label><br>\n                        <el-input class=\"search-code\" prefix-icon=\"el-icon-search\"></el-input>\n                    </el-col>\n    